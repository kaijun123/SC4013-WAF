    "express-fileupload": "^1.5.1",
    "form-data": "^4.0.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.13.1",
    "pgtools": "^1.0.1",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "toml": "^3.0.0",
    "uuid": "^11.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.5.1",
    "@types/node": "^20.12.11",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.1",
    "tsx": "^4.10.1",
    "typescript": "^5.4.5"
  }
